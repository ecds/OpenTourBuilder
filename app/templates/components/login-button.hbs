{{#if session.isAuthenticated}}
 {{#uk-button on-click=(action 'logOut')}}Logout{{/uk-button}}
{{else}}
{{#uk-modal modalName='ecds-login' as |modal|}}
  {{#modal.open}}
    Login
  {{/modal.open}}
  {{#modal.dialog}}
